    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: main.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Reporting Module",
            "type": "python",
            "request": "launch",
            "module": "reporting.main",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Add Buy",
            "type": "python",
            "request": "launch",
            "module": "admin.main",
            "console": "integratedTerminal",
            "args": ["--ticker=FIL/USD", "--op=add_buy_order", "--orders=dcb1c4bf-afdd-48b3-9195-59c719a15b5c"],
            "justMyCode": true
        },
        {
            "name": "Recon",
            "type": "python",
            "request": "launch",
            "module": "admin.main",
            "console": "integratedTerminal",     
            "args": ["--ticker_pairs=FIL/USD,SHIB/USD,COMP/USD,IOTX/USD,AVAX/USD,WCFG/USD,SOL/USD,ICP/USD,MANA/USD,DOGE/USD,BNT/USD,SNT/USD,ATOM/USD,DOT/USD,MATIC/USD,OXT/USD,AMP/USD,ASM/USD,LPT/USD,ACH/USD,LRC/USD,BAND/USD,ETH/USD,PLA/USD,BTC/USD,XLM/USD", "--op=recon", "--dry_run=False"],
       
            "justMyCode": true
        },
        {
            "name": "Get Order",
            "type": "python",
            "request": "launch",
            "module": "admin.main",
            "console": "integratedTerminal",     
            "args": ["--ticker=WCFG/USD", "--op=get_orders", "--orders=8986e12d-28a2-4d75-9f8f-959264bb5dd1"],
            "justMyCode": true
        },
        {
            "name": "Add Sell",
            "type": "python",
            "request": "launch",
            "module": "admin.main",
            "console": "integratedTerminal",
            "args": ["--ticker=IOTX/USD", "--op=add_sell_order", "--orders=efd94cb8-9e09-43bd-aabe-96888227f846"],
            "justMyCode": false
        },
        {
            "name": "Populate OHLCV",
            "type": "python",
            "request": "launch",
            "module": "admin.main",
            "console": "integratedTerminal",
            "args": ["--op=populate_ohlcv"],
            "justMyCode": false
        },
        {
            "name": "unittest",
            "type": "python",
            "request": "launch",
            "module": "unittest",
            "console": "integratedTerminal",
            "justMyCode": false
